<% include ../dashboard/menu.ejs %>

<!-- Main content -->
<div class="content-wrapper">

    <!-- Content area -->
    <div class="content">

 				<!-- Dashboard content -->
                 <div class="row">
					<!-- <div class="col-xl-4"> -->

						<!-- Marketing campaigns -->
						<div class="card">
							<div class="card-header header-elements-sm-inline">
								<h6 class="card-title">Statups</h6>
								<div class="header-elements">
									<span class="badge bg-success badge-pill">28 active</span>
									<div class="list-icons ml-3">
				                		<div class="list-icons-item dropdown">
				                			<a href="#" class="list-icons-item dropdown-toggle" data-toggle="dropdown"><i class="icon-menu7"></i></a>
											<div class="dropdown-menu">
												<a href="#" class="dropdown-item"><i class="icon-sync"></i> Update data</a>
												<a href="#" class="dropdown-item"><i class="icon-list-unordered"></i> Detailed log</a>
												<a href="#" class="dropdown-item"><i class="icon-pie5"></i> Statistics</a>
												<div class="dropdown-divider"></div>
												<a href="#" class="dropdown-item"><i class="icon-cross3"></i> Clear list</a>
											</div>
				                		</div>
				                	</div>
			                	</div>
							</div>


							<div class="table-responsive">
								<table class="table text-nowrap">
									<thead>
										<tr>
											<th>Startup</th>
                                            <th>Industry</th>
                                            <th>Business Model</th>
                                            <th>Growth Stage</th>
                                            <th>Funding</th>
											<th>Location</th>
											<th>Status</th>
											<th class="text-center" style="width: 20px;"><i class="icon-arrow-down12"></i></th>
										</tr>
									</thead>
									<tbody>
										<tr >
											<td colspan="5"><a class="btn btn-primary" href="/startup/create" role="button" btn-sm>New Startup</a></td>
                                        </tr>
                                        <% listStartups.forEach(function(startup){ %>

										<tr>
											<td>
												<div class="d-flex align-items-center">
													<div class="mr-3">
														<a href="#">
															<img src="/global_assets/images/image.png" class="rounded-circle" width="32" height="32" alt="">
														</a>
													</div>
													<div>
														<a href="/startup/edit/<%= startup.id %>" class="text-default font-weight-semibold"><%= startup.name %></a>
														<div class="text-muted font-size-sm">
															<span class="badge badge-mark border-blue mr-1"></span>
															<%= startup.description %>
														</div>
													</div>
												</div>
											</td>
                                            <td><span class="text-muted"><%= startup.industry %></span></td>
                                            <td><span class="text-muted"><%= startup.businessModel %></span></td>
											<td><span class="text-success-600"><i class="icon-stats-growth2 mr-2"></i> <%= startup.growthStage %></span></td>
                                            <td><h6 class="font-weight-semibold mb-0">$5,489</h6></td>
                                            <td><span class="text-muted"><%= startup.location %></span></td>
                                            <td><span class="badge bg-blue">Active</span></td>
                                            <td><a class="btn btn-danger" href="/startup/delete/<%= startup.id %>" role="button" btn-sm>Delete</a></td>
                                            <!-- <td><button type="button" class="btn btn-primary"><i class="mi-delete-forever"></i> Delete</button></td> -->
											<!-- <td class="text-center">
												<div class="list-icons">
													<div class="list-icons-item dropdown">
														<a href="#" class="list-icons-item dropdown-toggle caret-0" data-toggle="dropdown"><i class="icon-menu7"></i></a>
														<div class="dropdown-menu dropdown-menu-right">
															<a href="#" class="dropdown-item"><i class="icon-file-stats"></i> View statement</a>
															<a href="#" class="dropdown-item"><i class="icon-file-text2"></i> Edit campaign</a>
															<a href="#" class="dropdown-item"><i class="icon-file-locked"></i> Disable campaign</a>
															<div class="dropdown-divider"></div>
															<a href="#" class="dropdown-item"><i class="icon-gear"></i> Settings</a>
														</div>
													</div>
												</div>
											</td> -->
                                        </tr>
                                        <% }); %>

									</tbody>
								</table>
							</div>
						</div>
						<!-- /marketing campaigns -->


					<!-- </div> -->


				</div>
				<!-- /dashboard content -->



    </div>
    <!-- /content area -->

</div>
<!-- /main content -->

</div>
<!-- /page content -->